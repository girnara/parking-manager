{
  "queryType": "timeseries",
  "dataSource": "conversation_events",
  "intervals": "2017-01-01T00:00:00.000/2017-01-31T00:00:00.000",
  "granularity": "day",
  "descending": "true",
  "filter": { "type": "selector", "dimension": "tenantId", "value": "26" },
  "context": {
    "timeout": 180000
  },
  "aggregations": [
    {
      "name": "uniqueConversations",
      "type": "cardinality",
      "fieldNames": [
        "conversationId"
      ],
      "byRow": true
    }
  ],
  "threshold": 1000
}
